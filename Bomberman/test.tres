code = "shader_type canvas_item; 
uniform sampler2D frame0;
uniform float speed = 1.0;

void fragment() {
	
	float time = fract(TIME*speed);
	vec2 xd = UV * vec2(1.0, 1.0);
	
	float var = mod(floor(time/0.062), 6.0);
	xd += vec2(0.062*var, 0.0);
    COLOR = texture(frame0, xd);
}"

